{
  "name": "rtui",
  "version": "1.0.0",
  "description": "基于React+Typescript的UI组件库",
  "main": "index.js",
  "scripts": {
    "test": "jest --clearCache && jest",
    "dev": "webpack-dev-server",
    "build": "webpack"
  },
  "author": "liliswen",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.4",
    "@types/testing-library__jest-dom": "^5.0.3",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "style-loader": "^1.1.3",
    "svg-sprite-loader": "^4.2.1",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src/components/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "app/(.*).less": "identity-obj-proxy",
      "\\.(css|less)": "identity-obj-proxy",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testMocks/assetsMocks.js"
    },
    "testRegex": "^.+\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}